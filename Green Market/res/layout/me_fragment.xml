<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:scrollbars="none"
    android:background="#daeaf5"
    >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
 		android:background="#f5f7f8"
        android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/reely"
        android:layout_width="fill_parent"
        android:layout_height="200dp"
        android:background="@drawable/wall6"
        android:paddingLeft="15dp"
        android:paddingRight="15dp" >

        <com.integra.dealcaller.RoundedImageVieww2
            android:id="@+id/imageButton2"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="20dp"
            android:background="#00000000"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:padding="1dp"
            android:scaleType="fitXY"
            android:src="@drawable/null_profile"
            app:riv_border_color="#A6ffffff"
            app:riv_border_width="5dip"
            app:riv_corner_radius="100dip"
            app:riv_oval="false" />

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_below="@+id/imageButton2"
            android:background="#00000000" >

             <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" 
            android:id="@+id/lin"
             android:paddingLeft="5dp"
    		android:paddingRight="5dp"
            android:background="#00000000"
            android:layout_alignParentBottom="true"
        	android:layout_alignParentLeft="true"
        	android:layout_alignParentRight="true">
                 
    
     <ImageButton
        android:id="@+id/buttoso2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-1dip"
        android:layout_marginRight="-1dip"
        android:layout_weight="0.11"
        android:background="#16000000"
    	android:padding="3dp"
        
        android:src="@drawable/notification_icon"
        android:textColor="#ffffff" />
    
    <ImageButton
        android:id="@+id/buttoso3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dp"
        android:layout_marginLeft="-1dip"
        android:layout_marginRight="-1dip"
      	android:src="@drawable/update_status_icon"
        android:layout_weight="0.11"
        
      
        android:textColor="#ffffff"
        android:background="#16000000" />

    <ImageButton
        android:id="@+id/buttoso5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-1dip"
        android:layout_marginRight="-1dip"
        android:layout_weight="0.11"
        android:background="#16000000"
    	android:padding="3dp"
        
        android:src="@drawable/find_friends"
        android:textColor="#ffffff" />
    
     </LinearLayout>
       
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:background="#A3ffffff"
            android:paddingLeft="15dp"
            android:paddingRight="15dp" >
        </RelativeLayout>

    </RelativeLayout>
    
    <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:orientation="vertical"
            android:layout_marginTop="5dp" 
            android:id="@+id/lin88"
            android:layout_weight="0.11"
            android:background="#00000000"
            >
      <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
        
            android:id="@+id/lin2"
            
            android:background="#00000000"
            >
    
     <Button
        android:id="@+id/friend_request"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="0dip"
     	android:textSize="13sp"
        android:layout_weight="0.11"
        android:background="#3598db"
    	android:minHeight="100dp"
        android:text="Friend&#10;Requests"
        android:drawableTop="@drawable/friend_request_icon"
        android:paddingTop="10dp"
      
        android:textColor="#ffffff" />
                 
    
     <Button
        android:id="@+id/photos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="-1dip"
        android:layout_weight="0.11"
        android:background="#79bbe8"
    	android:minHeight="100dp"
        android:text="Photos&#10;"
        android:textSize="13sp"
        android:drawableTop="@drawable/photos_icon"
        android:paddingTop="10dp"
        android:textColor="#ffffff" />
    
    
    
     </LinearLayout>
     
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" 
            android:id="@+id/lin3"
           
            android:background="#00000000"
            >
    
     <Button
        android:id="@+id/groups"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-1dip"
        android:layout_marginRight="0dip"
        android:layout_weight="0.11"
        android:background="#79bbe8"
    	android:minHeight="100dp"
    	android:textSize="13sp"
        android:layout_marginTop="0dp"
        android:text="Talkers, &#10;Groups  "
        android:drawableTop="@drawable/talkers_icon"
        android:paddingTop="10dp"
        android:textColor="#ffffff" />
                 
    
     <Button
        android:id="@+id/schedules"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="-1dip"
        android:layout_weight="0.11"
        android:background="#3598db"
   		android:minHeight="100dp"
   		android:textSize="13sp"
        android:layout_marginTop="0dp"
        android:text="Download&#10;Basket"
        android:drawableTop="@drawable/talk_schedules_icon"
        android:paddingTop="10dp"
        android:textColor="#ffffff" />
    
    
    
     </LinearLayout>
     
         <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" 
            android:id="@+id/lin4"
           
            android:background="#00000000"
            >
    
     <Button
        android:id="@+id/edit_pocket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="0dip"
        android:layout_weight="0.11"
        android:background="#3598db"
    	android:minHeight="100dp"
        android:layout_marginTop="0dp"
        android:text="Edit &#10;Profile "
        android:textSize="13sp"
        android:drawableTop="@drawable/edit_icon"
        android:paddingTop="10dp"
        android:textColor="#ffffff" />
                 
    
     <Button
        android:id="@+id/talker_tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="-1dip"
        android:layout_weight="0.11"
        android:background="#79bbe8"
    	android:minHeight="100dp"
        android:layout_marginTop="0dp"
        android:textSize="13sp"
        android:text="Talker&#10;Tools"
        android:drawableTop="@drawable/talker_tools"
        android:paddingTop="10dp"
        android:textColor="#ffffff" 
        />
    
    
    
     </LinearLayout>
     
  
</LinearLayout>

</LinearLayout>
</ScrollView>